for iter in {1..5}; do
    echo $iter
    MODEL_NAME=WTMF_\(dataset_manual_0\.0\)_50\_$iter\_20_0\.01
    echo $MODEL_NAME
    twnews recommend --recommend_by_model $MODEL_NAME
    twnews recommend --eval
    echo ------------------------------------
done